    "@angular/router": "^21.0.0",
    "@supabase/supabase-js": "^2.89.0",
    "dexie": "^4.2.1",
    "gsap": "^3.14.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
